#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c;
    scanf("%f%f%f", &a,&b,&c);
    if(a<b && a<c)
        printf("1");
    else if(b<a && b<c)
        printf("2");
    else if(c<a && c<b)
        printf("3");
        
    if(a==b && b==c)
        printf("123");
    else if(a==b)
        printf("12");
    else if(b==c)
        printf("23");
    else if(a==c)
        printf("13");
    
    
    return 0;
}
