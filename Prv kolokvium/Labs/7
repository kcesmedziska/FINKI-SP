#include <stdio.h>
#include <stdlib.h>

int main()
{
    int km, v1=0,v2=0,p,m,c,d,z,r;
    float k,v;
    
    scanf("%d", &km);
    scanf("%d%d%f%d", &p,&m,&k,&c);
    scanf("%d%d%f%d", &d,&z,&v,&r);
    
    v1=p*m + (p-1)*c + km/k;
    v2=d*z + (d-1)*r + km/v;
    
    printf("%d %d\n", v1, v2);
    
    if(v1<v2)
        printf("1");
    else 
        printf("0");
    
    return 0;
}
