#include<stdio.h>
#include<stdlib.h>
#define max 100

int main()
{
    int n,a[max],i;
    scanf("%d", &n);
    for(i=0;i<n;i++){
        scanf("%d", &a[i]);
    }
    for(i=0;i<n;i++){
        printf("%d ", a[i]);
    }
    printf("\n");
    for(i=n-1;i>=0;i--){
        printf("%d ", a[i]);
    }
    printf("\n");
    for(i=n-1;i>=0;i--){
        if(i%2==0)
            printf("%d ", a[i]);
    }
    printf("\n");
    for(i=0;i<n;i++){
        if(i%2==0)
            printf("%d ", a[i]);
    }




    return 0;
}
