#include<stdio.h>
#include<stdlib.h>
#define max 100

int main()
{
    int i,n1[max],n2[max],skalar=0,n;
    scanf("%d", &n);
    for(i=0;i<n;++i)
        scanf("%d", &n1[i]);
    for(i=0;i<n;++i)
        scanf("%d", &n2[i]);
    for(i=0;i<n;++i)
        skalar=skalar + n1[i]*n2[i];

    printf("Skalarniot proizvod e %d", skalar);

    return 0;
}
